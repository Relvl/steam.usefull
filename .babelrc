{
    "presets": [
        "babel-preset-env",
        "babel-preset-es2015",
        "babel-preset-es2016",
        "babel-preset-es2017",
        "babel-preset-react",
        "babel-preset-stage-0",
        "babel-preset-stage-1",
        "babel-preset-stage-2",
        "babel-preset-stage-3",
        "babel-polyfill"
    ],
    "plugins": [
        "transform-decorators-legacy",
        "transform-es2015-modules-commonjs"
    ]
}